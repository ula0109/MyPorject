.text
main:	
	jal printMsg1
	jal printMsg2
	jal inputM	# return the result a0	
	mv s0,a0        # s0 mん
	jal printMsg3	# Set s0 equal to the result
	jal inputM
	mv s1,a0        # s1 い丁r
	jal printMsg4
	jal inputM
	mv s2,a0        # s2程幛@应r
	
	li s3,10         # s3=10
	
	#sky
	addi s5,s0,1    # s5=Sky
	jal printSky
	mv s9,s5
	jal prints9
	rem s5,s5,s3	#l计	
	mv a0,s5
	jal computeWuXin # computeWuXin(M=a0), return a0
        jal printResult #printResult(a0)	
	mv s5,t0
	#people
	add s6,s0,s1    # s6=people
	jal printPeople
	mv s9,s6
	jal prints9
	rem s6,s6,s3	#l计	
	mv a0,s6
	jal computeWuXin # computeWuXin(M=a0), return a0
        jal printResult #printResult(a0)
        mv s6,t0
        #land
        add s7,s2,s1    # s7=land
	jal printLand
	mv s9,s7
	jal prints9
	rem s7,s7,s3	#l计	
	mv a0,s7
	jal computeWuXin # computeWuXin(M=a0), return a0
        jal printResult #printResult(a0)
       mv s7,t0
         #outside
	addi s8,s2,1    # s8=outside
	jal printOutside
	mv s9,s8
	jal prints9
	rem s8,s8,s3	#l计	
	mv a0,s8
	jal computeWuXin # computeWuXin(M=a0), return a0
        jal printResult #printResult(a0)	
	mv s8,t0
	#total
	add s4,s0,s1     #s4=total
	add s4,s4,s2
	jal printTotal
	mv s9,s4
	jal prints9
	rem s4,s4,s3	#l计	
	mv a0,s4
	jal computeWuXin # computeWuXin(M=a0), return a0
        jal printResult #printResult(a0)
	mv s4,t0
	li t0,0
	li t1,1
	li t2,2
	li t3,3
	li t4,4
	li a3,-1
	li a4,-2
	li a5,-3
	li a6,-4
	#ぱ--Haa
	sub t5,s5,s6
	jal aa

	#ぱ--abb
	sub t5,s5,s7
	jal bb
	
	#ぱ--~cc
	sub t5,s5,s8
	jal cc
	
	#ぱ--`dd
	sub t5,s5,s4
	jal dd
	
	#H--aee
        sub t5,s6,s7
        jal ee
        #H--~ ff
        sub t5,s6,s8
        jal ff
        #H--`gg
        sub t5,s6,s4
        jal gg
        #a--~hh
        sub t5, s7,s8
        jal hh
        #a--`ii
        sub t5,s7,s4
        jal ii
        #~--`jj
        sub t5,s8,s4
        jal jj      
	j end

computeWuXin: 
	mv t1,a0

	addi sp,sp,-4
	sw ra,0(sp)
	
	blez t1,ModeWater #12(Wood),34(Fire),56(Earth),78(Metal),90(Water)	
	li a2,2	 
	ble t1,a2,ModeWood
	li a2,4
	ble t1,a2,ModeFire
	li a2,6	 
	ble t1,a2,ModeEarth
	li a2,8
	ble t1,a2,ModeMetal
	li a2,9
	ble t1,a2,ModeWater

endcomputeWuXin: 
	lw ra,0(sp)
	addi sp,sp,4
	ret  #return (a0)


ModeWood:
	la a0,wood
	li t0,0
	j endcomputeWuXin
	
ModeFire:
	la a0,fire
	li t0,1
	j endcomputeWuXin

ModeEarth:
	la a0,earth
	li t0,2
	j endcomputeWuXin

ModeMetal:
	la a0,metal
	li t0,3
	j endcomputeWuXin

ModeWater:
	la a0,water
	li t0,4
	j endcomputeWuXin

printResult:
	li a7, 4			# print string
 	ecall
 	ret
